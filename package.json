{
  "name": "koa-starter",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.1.0",
  "description": "Starter template with KoaJs.",
  "license": "MIT",
  "homepage": "https://github.com/Ardanas/koa-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ardanas/koa-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Ardanas/koa-starter/issues"
  },
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"nr dev:ts\" \"nr dev:node\"",
    "dev:ts": "tsup --watch --sourcemap",
    "dev:node": "nodemon ./dist/index.js",
    "build": "tsup",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.1",
    "koa-static": "^5.0.0",
    "mysql2": "^3.9.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@antfu/ni": "^0.21.12",
    "@types/koa": "^2.15.0",
    "@types/koa-router": "^7.4.8",
    "@types/koa-static": "^4.0.4",
    "@types/node": "^20.12.12",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
